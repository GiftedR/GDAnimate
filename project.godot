; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GDAnimator"
run/main_scene="res://CTR2AppRoot.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
run/low_processor_mode=true
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[dotnet]

project/assembly_name="GDAnimator"

[editor]

naming/node_name_num_separator=2
naming/default_signal_callback_name="_SGN_{node_name}_{signal_name}"
naming/default_signal_callback_to_self_name="_SGN_{signal_name}"
naming/scene_name_casing=1

[rendering]

textures/canvas_textures/default_texture_filter=0
